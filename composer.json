{
  "name": "ctasca/mage-bundle",
  "type": "magento2-module",
  "description": "Easily create Magento/Adobe Commerce PHP/XML/JS files from a set of templates",
  "license": [
    "MIT"
  ],
  "config": {
    "sort-packages": true
  },
  "version": "v3.4.0-BETA",
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Ctasca\\MageBundle\\": ""
    }
  },
  "scripts": {
    "post-install-cmd": "Ctasca\\MageBundle\\Composer\\PostInstall::copyTemplates"
  }
}
